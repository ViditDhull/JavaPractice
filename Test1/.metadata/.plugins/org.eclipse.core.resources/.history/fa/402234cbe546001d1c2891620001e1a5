import java.util.Scanner;

public class Table {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number : ");
		int num = s.nextInt();
		for(int i = 1; i <=10; i++) {
			int product = num * i;
			System.out.println(number + " X " + i " = " + product);
		}
	}

}
